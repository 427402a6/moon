<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">

<!-- StrokeThickness="0" -->
	<!-- Stretch="None" -->
		<!-- no width, no height -->
		<Rectangle Fill="#000000" Stroke="#FF0000" StrokeThickness="0" Stretch="None" Canvas.Left="10" Canvas.Top="10" />
		<!-- width only -->
		<Rectangle Fill="#000000" Stroke="#00FF00" StrokeThickness="0" Stretch="None" Canvas.Left="30" Canvas.Top="10" Width="10" />
		<!-- height only -->
		<Rectangle Fill="#000000" Stroke="#0000FF" StrokeThickness="0" Stretch="None" Canvas.Left="50" Canvas.Top="10" Height="15" />
		<!-- width and height -->
		<Rectangle Fill="#000000" Stroke="#FF00FF" StrokeThickness="0" Stretch="None" Canvas.Left="70" Canvas.Top="10" Width="10" Height="15" />
	<!-- Stretch="Fill" -->
		<!-- no width, no height -->
		<Rectangle Fill="#000000" Stroke="#FF0000" StrokeThickness="0" Stretch="Fill" Canvas.Left="110" Canvas.Top="10" />
		<!-- width only -->
		<Rectangle Fill="#000000" Stroke="#00FF00" StrokeThickness="0" Stretch="Fill" Canvas.Left="130" Canvas.Top="10" Width="10" />
		<!-- height only -->
		<Rectangle Fill="#000000" Stroke="#0000FF" StrokeThickness="0" Stretch="Fill" Canvas.Left="150" Canvas.Top="10" Height="15" />
		<!-- width and height -->
		<Rectangle Fill="#000000" Stroke="#FF00FF" StrokeThickness="0" Stretch="Fill" Canvas.Left="170" Canvas.Top="10" Width="10" Height="15" />
	<!-- Stretch="Uniform" -->
		<!-- no width, no height -->
		<Rectangle Fill="#000000" Stroke="#FF0000" StrokeThickness="0" Stretch="Uniform" Canvas.Left="210" Canvas.Top="10" />
		<!-- width only -->
		<Rectangle Fill="#000000" Stroke="#00FF00" StrokeThickness="0" Stretch="Uniform" Canvas.Left="230" Canvas.Top="10" Width="10" />
		<!-- height only -->
		<Rectangle Fill="#000000" Stroke="#0000FF" StrokeThickness="0" Stretch="Uniform" Canvas.Left="250" Canvas.Top="10" Height="15" />
		<!-- width and height -->
		<Rectangle Fill="#000000" Stroke="#FF00FF" StrokeThickness="0" Stretch="Uniform" Canvas.Left="270" Canvas.Top="10" Width="10" Height="15" />
	<!-- Stretch="UniformToFill" -->
		<!-- no width, no height -->
		<Rectangle Fill="#000000" Stroke="#FF0000" StrokeThickness="0" Stretch="UniformToFill" Canvas.Left="310" Canvas.Top="10" />
		<!-- width only -->
		<Rectangle Fill="#000000" Stroke="#00FF00" StrokeThickness="0" Stretch="UniformToFill" Canvas.Left="330" Canvas.Top="10" Width="10" />
		<!-- height only -->
		<Rectangle Fill="#000000" Stroke="#0000FF" StrokeThickness="0" Stretch="UniformToFill" Canvas.Left="350" Canvas.Top="10" Height="15" />
		<!-- width and height -->
		<Rectangle Fill="#000000" Stroke="#FF00FF" StrokeThickness="0" Stretch="UniformToFill" Canvas.Left="370" Canvas.Top="10" Width="10" Height="15" />

<!-- StrokeThickness smaller than width and height -->
